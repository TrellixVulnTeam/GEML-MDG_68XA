{
    "dataset_reader": {
        "type": "mds_reader",
        "token_indexers": {
            "tokens": {
                "type": "single_id",
                "token_min_padding_length": 4
            }
        },
        "tokenizer": {
            "word_splitter": {
                "type": "just_spaces"
            }
        }
    },
    "iterator": {
        "type": "basic",
        "batch_size": 32
    },
    "model": {
        "type": "basic_classifier_f1",
        "dropout": 0.5,
        "seq2vec_encoder": {
            "type": "cnn",
            "embedding_dim": 300,
            "ngram_filter_sizes": [
                2,
                3,
                4
            ],
            "num_filters": 128
        },
        "text_field_embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "embedding",
                    "embedding_dim": 300,
                    "pretrained_file": "process/fd_embedding.txt",
                    "trainable": true
                }
            }
        }
    },
    "train_data_path": "process/data_0305/train_0_0.1.pk",
    "validation_data_path": "process/data_0305/valid_0.pk",
    "trainer": {
        "cuda_device": 0,
        "grad_norm": 10,
        "num_epochs": 20,
        "optimizer": {
            "type": "adam",
            "lr": 0.001
        },
        "validation_metric": "+acc1"
    }
}